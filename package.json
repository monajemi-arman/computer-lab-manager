{
  "name": "computer-lab-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:nextJs\" \"npm run dev:ansibleApi\"",
    "dev:nextJs": "next dev --turbopack",
    "dev:ansibleApi": "python3 services/ansible-api/run.py",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate && prisma db push && tsx prisma/seed.ts"
  },
